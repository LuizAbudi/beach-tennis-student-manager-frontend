    "version": "0.2.0",
    "configurations": [
        {
            "name": "beach-tennis-student-manager-frontend",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "deviceId": "chrome",
            "args": [
              "--web-browser-flag",
              "--disable-web-security"
            ]
        },
        {
            "name": "beach-tennis-student-manager-frontend (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart",
            "deviceId": "chrome",
            "args": [
              "--web-browser-flag",
              "--disable-web-security"
            ]
        },
        {
            "name": "beach-tennis-student-manager-frontend (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "deviceId": "chrome",
            "args": [
              "--web-browser-flag",
              "--disable-web-security"
            ]
        }
    ]
}